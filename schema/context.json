{
    "xsd": "http://www.w3.org/2001/XMLSchema#",
    "dct": "http://purl.org/dc/terms/",
    "css": "http://www.w3.org/wiki/CSS/Properties/",

    "diograph": "http://diograph.org/schema",
    "diory": "diograph:diory",

    "ID": "@id",
    "name": "dct:title",
    "type": "@type",
    "IRI": {
        "@id": "dct:abstract",
        "@type": "@id"
    },
    "address": {
        "@id": "dct:source",
        "@type": "xsd:anyURI"
    },
    "background": "http://purl.org/dc/dcmitype/image",

    "style": "diograph:style",
    "date": {
        "@id": "dct:date",
        "@container": "@list"
    },
    "modified": {
        "@id": "dct:modified",
        "@type": "xsd:dateTime"
    },
    "created": {
        "@id": "dct:created",
        "@type": "xsd:dateTime"
    },

    "geo": "http://ld.geojson.org/vocab#",
    "location": "geo:location",
    "coordinates": {
        "@id": "geo:coordinates",
        "@container": "@list"
    },
    "Polygon": "geo:Polygon",
    "Point": "geo:Point",

    "position": "style:position",
    "status": "diograph:status",

    "doors": {
        "@id": "diograph:doors",
        "@container": "@list"
    },
    "avatars": {
        "@id": "diograph:agents",
        "@container": "@list"
    },
		"tools": {
				"@id": "diograph:tools",
				"@container": "@list"
		},
    "diorys": {
        "@id": "diograph:diorys",
        "@container": "@list"
    },
    "moments": {
        "@id": "diograph:moments",
        "@container": "@list"
    },
    "actions": {
            "@id": "diograph:actions",
            "@container": "@list"
    },
    "places": {
            "@id": "diograph:places",
            "@container": "@list"
    }
}